{
    "name": "demons",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon -r dotenv/config --exec babel-node src",
        "format": "prettier . --write --log-level=warn",
        "build": "npx babel src --out-dir ./build",
        "start": "node ./build",
        "demon": "pm2 start ecosystem.config.js --env production"
    },
    "dependencies": {
        "@babel/cli": "^7.25.6",
        "@babel/core": "^7.25.2",
        "@babel/node": "^7.25.0",
        "@babel/preset-env": "^7.25.4",
        "axios": "^1.7.7",
        "babel-plugin-module-resolver": "^5.0.2",
        "consola": "^3.2.3",
        "croner": "^8.1.2",
        "dotenv": "^16.4.5",
        "eventsource": "^2.0.2",
        "nodemon": "^3.1.7",
        "otpauth": "^9.3.4",
        "pm2": "^5.4.2",
        "prettier": "^3.3.3"
    },
    "engines": {
        "node": "20.12.2",
        "yarn": "1.22.22"
    }
}
